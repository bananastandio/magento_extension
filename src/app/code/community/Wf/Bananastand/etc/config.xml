<?xml version="1.0"?>
<config>
    <modules>
        <Wf_Bananastand>
            <version>1.0.0</version>
        </Wf_Bananastand>
    </modules>
    <global>
        <models>
            <banana>
                <class>Wf_Bananastand_Model</class>
            </banana>
        </models>
        <blocks>
            <banana>
                <class>Wf_Bananastand_Block</class>
            </banana>
        </blocks>
        <helpers>
            <banana>
                <class>Wf_Bananastand_Helper</class>
            </banana>
        </helpers>
        <events>
        </events>
    </global>
    <default>
        <banana>
            <general>
                <enabled>1</enabled>
                <public_key></public_key>
                <secret_key></secret_key>
            </general>
        </banana>
    </default>
    <frontend>
        <layout>
            <updates>
                <banana>
                    <file>banana.xml</file>
                </banana>
            </updates>
        </layout>
        <events>
        </events>
    </frontend>
</config>
